CREATE TABLE AchadosPerdidos
(
	id_obj 	SERIAL,
	local_obj VARCHAR(20),
	data_obj   VARCHAR(12),
	nome 		VARCHAR(40),
	status		VARCHAR(20),
	tipo		VARCHAR(20),
	observacao	VARCHAR(40),
	PRIMARY KEY (id_obj)
);
